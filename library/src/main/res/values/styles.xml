<resources>

    <declare-styleable name="PullActivateLayout">
        <attr name="actionViewId" format="reference" />
        <attr name="toolViewId" format="reference" />
    </declare-styleable>
    <item type="id" name="action_view" />
    <item type="id" name="tool_view" />
    <declare-styleable name="CircleImageView">
        <attr name="is_circle" format="boolean" />
        <attr name="is_cover_src" format="boolean" />
        <attr name="corner_radius" format="dimension" />
        <attr name="corner_top_left_radius" format="dimension" />
        <attr name="corner_top_right_radius" format="dimension" />
        <attr name="corner_bottom_left_radius" format="dimension" />
        <attr name="corner_bottom_right_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="inner_border_width" format="dimension" />
        <attr name="inner_border_color" format="color" />
        <attr name="mask_color" format="color" />
    </declare-styleable>

    <color name="normal">#dadada</color>
    <color name="pressed">#dd9025</color>
    <color name="disable">#818081</color>

    <declare-styleable name="LineText">
        <attr name="text" format="string" />
        <attr name="text_color" format="color" />
    </declare-styleable>
    <item type="id" name="center_text" />
    <item type="id" name="line_left" />

    <declare-styleable name="IconText">
        <attr name="it_desc" format="string" />
        <attr name="it_text" format="string" />
        <attr name="it_icon" format="integer" />
        <attr name="it_text_color" format="color" />
    </declare-styleable>
    <item type="id" name="left_icon" />
    <item type="id" name="left_text" />
    <item type="id" name="right_text" />


</resources>