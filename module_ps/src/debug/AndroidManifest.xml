<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ps">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:label="ps"
        android:icon="@drawable/emoji_00a9"
        android:theme="@style/Theme.AppCompat.Light"
        android:supportsRtl="true" >
        <activity android:name="com.season.example.PsActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.season.example.CropActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.season.example.BigGifActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.season.example.video.VideoActivity"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>